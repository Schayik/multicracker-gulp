body
    width: 100vw;
    overflow-x: hidden;
    background-color: $white;

    display: flex;
    flex-direction: column;
    justify-content: space-between;
    min-height: 100vh;

    font-family: 'Muli', sans-serif;

.compress
    max-width: $media.max;
    margin: 0 auto;
    padding: 0 $margin.max;


ul
    list-style: none;
    // padding: 0;
    margin: 0;

a
    color: $black;
    text-decoration: none;

h1, p
    padding: 0;
    margin: 0;